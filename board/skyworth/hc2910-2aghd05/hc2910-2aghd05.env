/*
 * Board env file for HC2910
 */

/* Simply reserve first 1MiB */
autostart=yes
fdt_addr_r=0x100000
fdtoverlay_addr_r=0x200000
fdtfile=hisilicon/hi3798mv200-hc2910-2aghd05.dtb
ipaddr=192.168.100.1
scriptaddr=0x300000
pxefile_addr_r=0x400000
kernel_addr_r=0x500000
kernel_comp_size=0x2B00000
kernel_comp_addr_r=0x3000000
ramdisk_addr_r=0x5000000

// The on-board router routes vlan10 to LAN port and vlan15 to WAN port
// Change this value or remove it if your board has tweaks
vlan=10

ipaddr=192.168.100.1
