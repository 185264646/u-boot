#define FDTFILE hisilicon/hc2910-2aghd05.dtb

/*
 * Note that the BootROM loads U-Boot at 0xC40000, at least don't touch
 * 0xC00000 - 0xC40000 because BL31 residents there
 */

scriptaddr=0xe00000
pxefile_addr_r=0xf00000
loadaddr=0x1000000
kernel_addr_r=${loadaddr}
fdt_addr_r=0x3000000
fdtfile=FDTFILE
ramdisk_addr_r=0x3100000
boot_targets=mmc

/* Bootmenu variables */
bootmenu_0=Download l-loader.bin from UART(XMODEM) and flash it to eMMC=loadx && mmc write 0 $loadaddr 800 || echo Failed to load l-loader.bin from UART, resetting... ; reset

