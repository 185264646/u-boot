#define FDTFILE hisilicon/hc2910-2aghd05.dtb
#define LOADADDR 0x1000000

/*
 * Note that the BootROM loads U-Boot at 0xC40000, at least don't touch
 * 0xC00000 - 0xC40000 because BL31 residents there
 */

autostart=yes
scriptaddr=0xe00000
pxefile_addr_r=0xf00000
loadaddr=LOADADDR
kernel_addr_r=LOADADDR
fdt_addr_r=0x3000000
fdtfile=FDTFILE
ramdisk_addr_r=0x3100000
boot_targets=mmc
